<%= form_for @research do |r| %>
  <%= r.label :title %>
  <%= r.text_field :title %>
  <%= r.label :category %>
  <%= r.text_field :category %>
  <%= r.label :description %>
  <%= r.text_field :description %>
  <%= r.label :body %>
  <%= r.text_field :body %>
  <%= r.label :author %>
  <%= r.text_field :author %>
  <%= r.submit "Create" %>
<% end %>
